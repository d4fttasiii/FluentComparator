<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp1.1;net461;net462</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>FluendComparator is a .NET library which allows you compare generic objects effectively. 
It makes it easy to exclude certain properties from the comparison process and it can return the list of differences.</Description>
    <RepositoryUrl>https://github.com/petertasner/FluentComparator</RepositoryUrl>
    <PackageProjectUrl>https://github.com/petertasner/FluentComparator</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/petertasner/FluentComparator/blob/master/LICENSE.md</PackageLicenseUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageTags>fluent netcore compare comparison</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

</Project>